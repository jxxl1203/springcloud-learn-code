spring:
  application:
    name: config
  cloud:
    config:
      #uri: http://localhost:8888/
      profile: default
      label: master
      discovery: 
        enabled: true
        serviceId: config-server
      
server:
  port: 8880
  
eureka: 
  client: 
    serviceUrl: 
      defaultZone: http://localhost:8081/eureka/

